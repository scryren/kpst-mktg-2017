      <div class="col-12 col-md-4 col-lg-3 text-center pb-3 pb-md-0">
        <img src="${mkto-presenter-5-img}" alt="" class="img-fluid rounded-circle" style="max-width: 150px;">
      </div>

      <div class="col-12 col-md-8 col-lg-7 text-center text-md-left">

        <div class="row">
          <div class="col-12">
            <p class="presenter-name">${mkto-presenter-5-name}</p>
            <p class="presenter-title">${mkto-presenter-5-title}</p>
          </div>
        </div>

        <div class="row">
          <div class="col-12 mktoText" id="mkto-presenter-5-bio" mktoName="Presenter 5 Bio">
            ${mkto-presenter-5-copy}
          </div>
        </div>

      </div>

    </div>

  </div><!--End Presenter Group-->
</div>

<div id="form-topper" class="container-fluid" >
  <div class="container">
    <div class="row">
      <div class="col-12 text-center">
        <img src="http://resources.kapost.com/rs/389-IOX-111/images/kpst-icon-small-webinar-170x170.png" class="img-fluid" style="max-height: 100px;">
        <h4>Register for this webinar!</h4>
      </div>
    </div>
  </div>
</div>

<div id="form-wrapper" class="container-fluid">
  <div class="container">
    <section id="form-container">
      <div class="row no-gutters justify-content-center">
        <div class="col-12 col-lg-6 col-xl-5">
          <%= partial "partials/form-vertical" %>
        </div>
      </div>
    </section>
  </div>
</div>
