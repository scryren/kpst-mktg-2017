---
title: Kapost
layout: layout-webinar
---

<div id="title" class="container-fluid">
  <div class="container">

    <div class="row text-center">
      <div class="col-12">
        <h1>${mkto-header-copy}</h1>
        <h4>${mkto-date} | ${mkto-time}</h4>
      </div>
    </div>
  </div>
</div>

<div id="content-summary" class="container-fluid" >
  <div class="container">

    <div class="row">

      <div class="col-12 col-lg-5 text-center">
          <img src="${mkto-img-main}" class="img-fluid">
      </div>

      <div class="col-10 col-lg-6 col-xl-6 mx-auto mt-5 align-self-center">

        ${mkto-summary-copy}

        <a href="#kapostmktoform"><button type="button" class="btn btn-primary">${mkto-summary-button}</button></a>

      </div>
    </div>

  </div>
</div>


<div id="content" class="container-fluid">
  <div class="container">
    <div class="row justify-content-center">
      <div class="col-12 col-lg-9">
        ${mkto-content-copy}
      </div>
    </div>
  </div>
</div>

<div id="content-presenter" class="container">
  <div class="container">

    <div class="row">
      <div class="col-12 text-center">
        <h3>Your Hosts</h3>
      </div>
    </div>

    <!--Presenter 1-->
    <div class="row presenter align-items-center justify-content-center" style="display: ${mkto-presenter-1-display};">

      <div class="col-12 col-md-4 col-lg-3 text-center pb-3 pb-md-0">
        <img src="${mkto-presenter-1-img}" alt="" class="img-fluid rounded-circle" style="max-width: 150px;">
      </div>

      <div class="col-12 col-md-8 col-lg-7 text-center text-md-left">

        <div class="row">
          <div class="col-12">
            <p class="presenter-name">${mkto-presenter-1-name}</p>
            <p class="presenter-title">${mkto-presenter-1-title}</p>
          </div>
        </div>

        <div class="row">
          <div class="col-12 mktoText" id="mkto-presenter-1-bio" mktoName="Presenter 1 Bio">
            ${mkto-presenter-1-copy}
          </div>
        </div>

      </div>

    </div>

    <!--Presenter 2-->
    <div class="row presenter align-items-center justify-content-center" style="display: ${mkto-presenter-2-display};">

      <div class="col-12 col-md-4 col-lg-3 text-center pb-3 pb-md-0">
