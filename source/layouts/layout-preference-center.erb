<!doctype html>
<html>
  <head>
    <meta content="IE=edge" http-equiv="X-UA-Compatible">
    <meta charset="utf-8">
    <meta content="width=device-width,initial-scale=1.0,minimum-scale=1.0,maximum-scale=1.0,user-scalable=no" name="viewport">

    <%= partial "partials/ga" %>

    <%= partial "partials/mkto-variables-main" %>
    <%= partial "partials/mkto-variables-form" %>

    <!-- Use title if it's in the page YAML frontmatter -->
    <title>Kapost</title>

    <script src="https://code.jquery.com/jquery-3.1.1.min.js"></script>

    <!--Referral Source Code-->
    <script src="https://kapost.com/assets/tracking/url.js"></script>
    <script src="https://kapost.com/assets/tracking/js.cookie.js"></script>
    <script src="https://kapost.com/assets/tracking/jquery.refsource.1.3.js"></script>

    <%= javascript_include_tag 'jquery.privacy-statement-kpst' %>


    <link rel="stylesheet" href="http://resources.kapost.com/rs/389-IOX-111/images/bootstrap.min-4.0.0.css">
    <script src="http://resources.kapost.com/rs/389-IOX-111/images/bootstrap.min-4.0.0.js"></script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css">

    <%# javascript_include_tag 'floatlabels' %>
    <script src="http://resources.kapost.com/rs/389-IOX-111/images/floatlabels-textarea-select-support.js"></script>

    <!--Floating Form Labels-->
    <script>
      $(document).ready(function () {
        $('.floatlabel').floatlabel();
      });
    </script>

    <!--<link href="http://resources.kapost.com/rs/389-IOX-111/images/2017-Q2-KPST.css" rel="stylesheet">-->
    <%= stylesheet_link_tag '2017-Q3-KPST' %>

  </head>

  <body class="<%= page_classes %>">
    <%= partial "partials/header-no-menu" %>
      <%= yield %>
      <%= partial "partials/footer" %>
    <%=  partial "partials/footer-tracking" %>
  </body>

</html>
